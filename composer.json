{
  "name": "negreanucalin/sparkle-dto",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Negreanu Calin",
      "email": "negreanu0calin@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "SparkleDto\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SparkleDto\\Tests\\": "tests/"
    }
